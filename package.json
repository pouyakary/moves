{
  "name": "move",
  "displayName": "Cursor Moves",
  "description": "Moves your cursor under the columns in the above line",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
     "*"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "move.cursorUnderNextColumn",
        "title": "Move Cursor Under Next Column"
      },
      {
        "command": "move.cursorUnderPreviousColumn",
        "title": "Move Cursor Under Previous Column"
      }
    ],
    "keybindings": [
        {
            "command":     "move.cursorUnderNextColumn",
            "key":         "cmd+right",
            "when":        "editorTextFocus"
        },
        {
            "command":     "move.cursorUnderPreviousColumn",
            "key":         "cmd+left",
            "when":        "editorTextFocus"
        }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "yarn run compile-web",
    "vscode:prepublish": "yarn run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/mocha": "^9.1.1",
    "eslint": "^8.20.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-web": "^0.0.29",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@types/webpack-env": "^1.17.0",
    "assert": "^2.0.0",
    "process": "^0.11.10"
  }
}
